<?xml version="1.0"?>
<Container version="2">
    <Name>VeracryptGUI</Name>
    <Repository>dcflachs/veracrypt-gui</Repository>
    <Registry>https://hub.docker.com/r/dcflachs/veracrypt-gui</Registry>
    <Network>bridge</Network>
    <MyIP/>
    <Shell>sh</Shell>
    <Privileged>true</Privileged>
    <Support>https://forums.unraid.net/topic/173449-support-cyberschorschs-templates-flowise/</Support>
    <Project>https://github.com/dcflachs/docker-containers</Project>
    <Overview>Use Veracrypt Containers in Docker</Overview>
    <Category>Security: Tools:</Category>
    <WebUI>http://[IP]:[PORT:5800]</WebUI>
    <TemplateURL/>
    <Icon>https://veracrypt.fr/en/VeraCrypt128x128.png</Icon>
    <ExtraParams/>
    <PostArgs/>
    <CPUset/>
    <DateInstalled/>
    <DonateText/>
    <DonateLink/>
    <Requires/>
    <Config Name="Config Path" Target="/config" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/appdata/veracrypt-gui/config/</Config>
    <Config Name="Containers" Target="/mnt/containers" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/data/veracrypt-containers</Config>
    <Config Name="Images" Target="/mnt/disks" Default="" Mode="rw,shared" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/data/veracrypt-images</Config>
    <Config Name="DISPLAY_WIDTH" Target="DISPLAY_WIDTH" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1280</Config>
    <Config Name="DISPLAY_HEIGHT" Target="DISPLAY_HEIGHT" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">720</Config>
    <Config Name="GROUP_ID" Target="GROUP_ID" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">100</Config>
    <Config Name="USER_ID" Target="USER_ID" Default="1000" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
    <Config Name="Web UI" Target="5800" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">5800</Config>
    <Config Name="Mounted Containers" Target="/media" Default="" Mode="rw,shared" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/data/veracrypt-mounts</Config>
</Container>